# 智谱AI API Key
ZHIPUAI_API_KEY=your_zhipuai_api_key_here

# OpenRouter API Key
OPENROUTER_API_KEY=your_openrouter_api_key_here

# Cerebras API Key (推荐默认使用)
CEREBRAS_API_KEY=your_cerebras_api_key_here
# Cerebras API URL
CEREBRAS_API_URL=https://api.cerebras.ai/v1
# Cerebras可用模型
CEREBRAS_MODELS=qwen-3-235b-a22b-instruct-2507,gpt-oss-120b
# Cerebras默认模型
CEREBRAS_DEFAULT_MODEL=qwen-3-235b-a22b-instruct-2507

# JWT密钥
JWT_SECRET=development_jwt_secret_key_for_local_testing_only

# 管理员用户名
ADMIN_USERNAME=admin

# 管理员密码
ADMIN_PASSWORD=admin123

# 开发环境标识
NODE_ENV=development

# LLM 配置
DEFAULT_LLM_PROVIDER=cerebras
ENABLE_LLM_FALLBACK=true